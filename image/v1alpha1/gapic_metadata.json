                "CreateFolder"
              ]
            },
            "DeleteFolder": {
              "methods": [
                "DeleteFolder"
              ]
            },
            "GetFolder": {
              "methods": [
                "GetFolder"
              ]
            },
            "GetFolderSettings": {
              "methods": [
                "GetFolderSettings"
              ]
            },
            "GetImage": {
              "methods": [
                "GetImage"
              ]
            },
            "ImportImage": {
              "methods": [
                "ImportImage"
              ]
            },
            "ListFolders": {
              "methods": [
                "ListFolders"
              ]
            },
            "UpdateFolderSettings": {
              "methods": [
                "UpdateFolderSettings"
              ]
            },
            "UploadImage": {
              "methods": [
                "UploadImage"
              ]
            }
          }
        }
      }
    },
    "ImageRouter": {
      "clients": {
        "grpc": {
          "libraryClient": "ImageRouterClient",
          "rpcs": {
